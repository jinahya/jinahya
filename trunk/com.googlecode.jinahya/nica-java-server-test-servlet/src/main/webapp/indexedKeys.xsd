<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified"
           version="1.0"
           targetNamespace="http://jinahya.googlecode.com/nica/test"
           xmlns:tns="http://jinahya.googlecode.com/nica/test"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="indexedKeys" type="tns:indexedKeys"/>

  <xs:complexType name="indexedKeys">
    <xs:sequence>
      <xs:element name="indexedKey" type="tns:indexedKey" minOccurs="0"
                  maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="indexedKey">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="index" type="xs:ID" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
</xs:schema>