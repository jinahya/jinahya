  <developers>
    <developer>
      <id>jinahya</id>
      <name>Jin Kwon</name>
      <email>jinahya@gmail.com</email>
      <roles>
        <role>everything</role>
      </roles>
      <timezone>+9</timezone>
      <properties>
      </properties>
    </developer>
  </developers>
  <contributors>
  </contributors>

  <!--issueManagement/-->
  <!--ciManagement/-->
  <!--mailingLists/-->
  <scm>
    <connection>scm:svn:http://jinahya.googlecode.com/svn/maven/2/proj/${project.groupId}/${project.artifactId}/trunk/</connection>
    <developerConnection>scm:svn:http://jinahya.googlecode.com/svn/maven/2/proj/${project.groupId}/${project.artifactId}/trunk/</developerConnection>
    <tag>HEAD</tag>
    <url>http://code.google.com/p/jinahya/source/browse/maven/2/proj/${project.groupId}/${project.artifactId}/trunk/</url>
  </scm>
  <!--prerequisites/-->
  <repositories>
